    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "mapbox-gl": "^2.7.0",
    "mongoose": "^6.2.3"
  },
  "devDependencies": {
    "currently": "^0.0.8",
